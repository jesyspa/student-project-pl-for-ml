(define s "")
(read-line s)

(define chars (to-list s))
(define lower 0)
(define upper 0)
(define i 0)
(define ch "")
(define ch-code 0)

(while (< i (length chars))
    (set ch (get chars i))
    (set ch-code (get-ascii ch))
    (if (>= ch-code 97)
        (set lower (+ lower 1))
        (set upper (+ upper 1))
    )
    (set i (+ i 1))
)

(if (> upper lower)
    (print (to-upper s))
    (print (to-lower s))
)